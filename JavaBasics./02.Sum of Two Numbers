import java.util.Scanner;

public class Main {
    public static void main(String[] args) {


        Scanner scanner = new Scanner(System.in);

        int x = 0;
        int count = 0;
        int interval = Integer.parseInt(scanner.nextLine());
        int end = Integer.parseInt(scanner.nextLine());
        int magicNum = Integer.parseInt(scanner.nextLine());
        for (int i = interval; i <= end; i++) {
            for (int j = interval; j <= end; j++) {
                count++;
                if (i + j == magicNum) {
                    System.out.printf("\nCombination N:%d (%d + %d = %d)", count, i, j, magicNum);
                x=1;  
                break;
                }
            }
            if (x!= 0) {
                break;
            }
        }
        if (x==0){
            System.out.printf("\n%d combinations - neither equals %d",count,magicNum);
        }
    }
}


import java.util.Scanner;

public class Main {
    public static void main(String[] args) {


        Scanner scanner = new Scanner(System.in);
       String actorName = scanner.nextLine();
       double pointAcademy = Double.parseDouble(scanner.nextLine());
       int n =Integer.parseInt(scanner.nextLine()); //Amount judges


        for (int i = 0; i < n; i++) {
                String judgeName = scanner.nextLine();
                double pointJudge = Double.parseDouble(scanner.nextLine());
                pointAcademy += (judgeName.length() * pointJudge) / 2;
                if (pointAcademy > 1250.5) {
                    System.out.printf("Congratulations, %s got a nominee for leading role with %.1f!", actorName, pointAcademy);
                 break;
                }

        }
        double pointsLeft = Math.abs(pointAcademy-1250.5);
        if (pointAcademy<1250.5){
            System.out.printf("Sorry, %s you need %.1f more!",actorName,pointsLeft);
        }


    }
}

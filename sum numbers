import javax.swing.*;
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {


        Scanner scanner = new Scanner(System.in);

        int number= scanner.nextInt();
        int sum=0;
        for (int i = 0; i < number; i++) {
            int numbers = scanner.nextInt();
            sum+=numbers;
        }
        System.out.println(sum);




     }
}

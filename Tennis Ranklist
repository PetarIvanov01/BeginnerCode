
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {


        Scanner scanner = new Scanner(System.in);

        int amountOfTournaments = Integer.parseInt(scanner.nextLine());
        double points = Double.parseDouble(scanner.nextLine());
        double sumPoints=0 ;
        double averagePoints=0;
        double wintournaments = 0;
        for (int i = 0; i < amountOfTournaments; i++) {
            String stage = scanner.nextLine();
            if (stage.equals("W")){
              sumPoints+=2000;
              wintournaments++;
            } else if (stage.equals("F")) {
                sumPoints+=1200;
            } else if (stage.equals("SF")) {
                sumPoints+=720;
            }
        }

        averagePoints=sumPoints/amountOfTournaments;
        sumPoints+=points;
        System.out.printf("Final points: %.0f\n" ,sumPoints );
        System.out.printf("Average points: %d\n ",(int) averagePoints);
        System.out.printf("%.2f%%\n",wintournaments/amountOfTournaments*100);
    }
}

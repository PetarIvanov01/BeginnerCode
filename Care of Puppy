import java.util.Scanner;

public class Main {
    public static void main(String[] args) {


        Scanner scanner = new Scanner(System.in);

        int kgFood = Integer.parseInt(scanner.nextLine())*1000;
        String input = scanner.nextLine();
        int sumfood=0;
       while (!input.equals("Adopted")){
           int grFoodEaten = Integer.parseInt(input);
           sumfood+=grFoodEaten;
           input = scanner.nextLine();
       }
       if (sumfood<=kgFood){
           int diff = kgFood-sumfood;
           System.out.printf("Food is enough! Leftovers: %d grams.",diff);
       } else {
           int diff = sumfood -kgFood;
           System.out.printf("Food is not enough. You need %d grams more.",Math.abs(diff));
       }




    }
}

function Titles (input){
let age = Number(input[0]);
let gender = String(input[1]);
 if (age >= 16){
   switch (gender){
    case `m` : console.log(`Mr.`)
    break;
    case `f` : console.log(`Ms.`)
    break;
   }
}
else {
    switch(gender){
        case `m` : console.log(`Master`)
        break;
        case `f` : console.log(`Miss`)
        break;
    }
}
}
